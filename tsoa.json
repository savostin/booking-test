{
    "entryFile": "src/index.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "spec": {
        "securityDefinitions": {
            "apiKey": {
                "type": "apiKey",
                "name": "X-API-KEY",
                "in": "header"
            }
        },
        "outputDirectory": "src/api/swagger",
        "specFileBaseName": "v1",
        "specVersion": 3
    },
    "routes": {
        "authenticationModule": "./api/middlewares/auth.ts"
    }
}